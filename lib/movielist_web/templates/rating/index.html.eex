<h1>Listing Ratings</h1>

<table>
  <thead>
    <tr>
      <th>Date scored</th>
      <th>Score</th>

      <th></th>
    </tr>
  </thead>
  <tbody>
<%= for rating <- @ratings do %>
    <tr>
      <td><%= rating.date_scored %></td>
      <td><%= rating.score %></td>

      <td>
        <%= link "Show", to: Routes.rating_path(@conn, :show, rating) %>
        <%= link "Edit", to: Routes.rating_path(@conn, :edit, rating) %>
        <%= link "Delete", to: Routes.rating_path(@conn, :delete, rating), method: :delete, data: [confirm: "Are you sure?"] %>
      </td>
    </tr>
<% end %>
  </tbody>
</table>

<span><%= link "New Rating", to: Routes.rating_path(@conn, :new) %></span>
